cdc <- read.csv("https://aggreenbean.github.io/Course-Slides/Bios%20600/labs/data/cdc_cleaned.csv")
View(cdc)
getwd()
cdc |>
ggplot(aes(x = Exercise, y = Obesity)) +
geom_point() +
facet_grid(. ~ HDI) +
ggtitle("Adequate aerobic activity \n associated with lower obesity") +
xlab("Adequate aerobic activity (%)") +
theme(text = element_text(size = 26),
axis.text = element_text(size = 16))
